{
  "name": "rotineira-front-end",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "coveragePathIgnorePatterns": [
      "node_modules",
      "setupTests.js",
      "<rootDir>/src/index.js",
      "<rootDir>/src/theme.js",
      "<rootDir>/src/env.js",
      "<rootDir>/src/App.js",
      "<rootDir>/src/store",
      "<rootDir>/src/assets",
      "<rootDir>/src/i18n",
      "<rootDir>/src/services",
      "styled-wrapper.js",
      "styled-wrappers.js",
      "actions.js",
      "reducer.js",
      "reducers.js",
      "rootReducer.js",
      "sagas.js",
      "rootSaga.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.5.0",
    "file-saver": "^2.0.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toastify": "^6.0.5",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "socket.io-client": "^2.3.0",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --watchAll=false --coverage --no-cache",
    "test-coverage": "react-app-rewired test --env=jsdom --watchAll=false --coverage --no-cache",
    "eject": "react-scripts eject",
    "storybook": "start-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "babel-loader": "8.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.0.5"
  }
}
